    "sass": "^1.54.5",
    "terser": "^5.14.2",
    "vite": "^3.2.4",
    "vite-plugin-istanbul": "^3.0.1",
    "vitest": "^0.27.2",
    "vue-template-compiler": "^2.7.8"
  }
}
